{
  "name": "tolya-electron",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "xo",
	"build": "electron-packager . myApp --all --version=1.3.4 --app-version=1.0.0 --out=dist --ignore=dist --prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolyaganzin/Tolya-electron"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "homepage": "https://github.com/tolyaganzin/Tolya-electron",
  "devDependencies": {
    "electron-prebuilt": "1.2.0",
    "xo": "0.16.0"
  },
  "xo": {
    "space": 2,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "dist/**"
    ]
  }
}
